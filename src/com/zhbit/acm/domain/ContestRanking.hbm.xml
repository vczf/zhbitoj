<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zhbit.acm.domain.ContestRanking" table="contest_rank_info">
        <composite-id>
            <key-property name="problemId" column="problem_id" />
            <key-property name="contestId" column="contest_id" />
            <key-property name="userName" column="user_name" />
        </composite-id>
        <property name="acTime" column="in_date" />
        <property name="submit" column="submit" />
    </class>


</hibernate-mapping>